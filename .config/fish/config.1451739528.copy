set -g VAGRANT_HOME /Volumes/sdcard/.vagrant.d/
set -g ANDROID_HOME $HOME/Library/Android/sdk
set -g PATH $PATH $ANDROID_HOME/tools $ANDROID_HOME/platform-tools

set -e fish_key_bindings
set -U fish_key_bindings fish_vi_key_bindings
set fish_path $HOME/.oh-my-fish
set -e fish_theme budspencer_nogreeting
set budspencer_colors 000000 333333 666666 ffffff ffff00 ff6600 ff0000 ff0033 3300ff 0000ff 00ffff 00ff00
. $fish_path/oh-my-fish.fish
